//Faça um programa que converta a temperatura dada em Fahrenheit para Celsius.  Para testar se a sua resposta está correta saiba que 100ºC = 212F. Considere C/5 = (F-32)/9

import java.util.Scanner;
public class Main{
    public static void main(String[] args){
        Double A,B,C,Delta,y1,y2;
        Scanner reader=new Scanner(System.in);
        System.out.println("Digite o valor de A: ");
        A=reader.nextDouble();
        System.out.println("Digite o valor de B: ");
        B=reader.nextDouble();
        System.out.println("Digite o valor de C: ");
        C=reader.nextDouble();
        Delta=(B*B)-4*A*C;
        System.out.println(Delta);
        y1=-B(+Math.sqrt(Delta))/2*A;
        y2=-B(-Math.sqrt(Delta))/2*A;
        System.out.println("O valor da primeira raiz é "+y1+" e a segunda raiz é "+y2);
        
        
    }
}
